{
  "name": "dataops-gateway",
  "targets": {
    "serve": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "yarn start:dev",
        "cwd": "apps/dataops-gateway"
      }
    },
    "build": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "yarn build",
        "cwd": "apps/dataops-gateway"
      }
    },
    "deploy": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "npx serverless deploy",
        "cwd": "apps/dataops-gateway"
      }
    }
  }
}